<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homepage1">
    <TextView
        android:layout_width="match_parent"
        android:id="@+id/pesttext"
        android:textSize="22sp"
        android:text="@string/www_kpest"
        android:layout_marginTop="10dp"
        android:layout_height="80dp"/>

    <ImageView
        android:id="@+id/imagepest"
        android:layout_width="350dp"
        android:layout_height="289dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:layout_marginBottom="320dp"
        android:background="@drawable/backshape"
        android:fitsSystemWindows="true"
        android:src="@drawable/fumi" />

    <ImageView
        android:id="@+id/imgepest2"
        android:layout_width="370dp"
        android:src="@drawable/control"
        android:background="@drawable/backshape"
        android:layout_height="232dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:layout_marginBottom="49dp" />


</RelativeLayout>